{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "React App",
	"compilerOptions": {
		"allowJs": true,
		"declaration": false,
		"declarationMap": false,
		"incremental": true,
		"jsx": "preserve",
		"lib": ["dom", "dom.iterable", "esnext"],
		"module": "ESNext",
		"noEmit": true,
		"resolveJsonModule": true,
		"target": "es5"
	}
}
